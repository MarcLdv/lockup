# Configuration du serveur
PORT=3000
NODE_ENV=development

# Base de données PostgreSQL
# Option 1: URL complète
DATABASE_URL=postgresql://lockup:password@localhost:5432/lockup

# Option 2: Variables séparées (si DATABASE_URL non défini)
# PGHOST=localhost
# PGPORT=5432
# PGUSER=lockup
# PGPASSWORD=password
# PGDATABASE=lockup
# PGSSL=false

# JWT
JWT_SECRET=changez-moi-en-production-avec-une-cle-longue-et-aleatoire
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:8081,http://localhost:19006
